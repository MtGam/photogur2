<h1>Add a Photo</h1>
<%= link_to "Back to Doll Fotos", dollfotos_url %>

<%= form_for @dollfoto do |f| %>
<p>
  <%= f.label :artist %><br>
  <%= f.text_field :artist %>
</p>
<p>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :url %><br>
  <%= f.text_field :url %>
</p>
<p>
  <%= submit_tag "Save" %>
</p>
<% end %>
